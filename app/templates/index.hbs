<div class="container">
<div class = "product-grid">
	{{#each model as |product|}}
	<div class="col-md-6 product-container">
		<div class="product-img">
			<img src = "{{product.imageUrl}}">
		</div>
		<div class="product-content">
			<h4>{{product.title}} for <b>Rs {{product.price}}</b></h4>
		</div>
	</div>
	{{/each}}
	</div>
</div>